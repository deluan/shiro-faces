             <handler-class>org.apache.shiro.web.faces.facelets.ShiroComponentHandler</handler-class>
        </component>
    </tag>

    <tag>
        <tag-name>notAuthenticated</tag-name>
        <component>
            <component-type>org.apache.shiro.web.faces.component.notauthenticated.NotAuthenticated</component-type>
             <renderer-type>org.apache.shiro.web.faces.component.notauthenticated.NotAuthenticatedRenderer</renderer-type>
             <handler-class>org.apache.shiro.web.faces.facelets.ShiroComponentHandler</handler-class>
        </component>
    </tag>

    <tag>
        <tag-name>hasRole</tag-name>
        <component>
            <component-type>org.apache.shiro.web.faces.component.hasrole.HasRole</component-type>
             <renderer-type>org.apache.shiro.web.faces.component.hasrole.HasRoleRenderer</renderer-type>
             <handler-class>org.apache.shiro.web.faces.facelets.ShiroComponentHandler</handler-class>
        </component>
        <attribute>
