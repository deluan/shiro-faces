    <tag>
        <tag-name>lacksRole</tag-name>
        <component>
            <component-type>org.apache.shiro.web.faces.component.lacksrole.LacksRole</component-type>
             <renderer-type>org.apache.shiro.web.faces.component.lacksrole.LacksRoleRenderer</renderer-type>
             <handler-class>org.apache.shiro.web.faces.facelets.ShiroComponentHandler</handler-class>
        </component>
        <attribute>
            <description></description>
            <name>name</name>
            <required>true</required>
        </attribute>
    </tag>

    <tag>
        <tag-name>hasAnyRoles</tag-name>
        <component>
            <component-type>org.apache.shiro.web.faces.component.hasanyroles.HasAnyRoles</component-type>
             <renderer-type>org.apache.shiro.web.faces.component.hasanyroles.HasAnyRolesRenderer</renderer-type>
             <handler-class>org.apache.shiro.web.faces.facelets.ShiroComponentHandler</handler-class>
        </component>
        <attribute>
            <description></description>
            <name>name</name>
            <required>true</required>
        </attribute>
    </tag>

</facelet-taglib>

